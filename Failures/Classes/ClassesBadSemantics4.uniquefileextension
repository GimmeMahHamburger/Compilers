class TinMan{
	int legs
	int arms
	char $ name[10]
	bool brain
	
}
class Witch{
	int wicked
	bool east
	int dead
	void <- bool status :setDead{
		dead=status
	}
	void <- :house{
		__true setDead _east if	
	}
}

TinMan Gregory
Gregory.brain='h'
Gregory.name=20000000.1
Witch Gertrude
Gertrude.east=true
Gertrude.house
_Gertrude.dead write

~~~expected output
1
~~~
