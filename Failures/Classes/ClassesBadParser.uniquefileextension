}
class nope Witch{
	bool  extraneous wicked
	east
	bool dead
	void <- bool status :setDead{
		dead=status
	}
	void <- :house{
		__true setDead _east if	
	}
}

TinMan Gregory
Gregory.brain=false
Gregory.name='not greg'
